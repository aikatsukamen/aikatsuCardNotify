{
    "log4js":
    {
        "appenders":
        {
            "file":
            {
                "type": "dateFile",
                "filename": "./log/system.log",
                "pattern": "-yyyy-MM-dd",
                "backups": 5
            }
        },
        "categories":
        {
            "default":
            {
                "appenders": ["file"],
                "level": "info"
            }
        }
    }
}