{
  "log4js": {
    "appenders": {
      "system": {
        "type": "dateFile",
        "filename": "./log/system.log",
        "pattern": "-yyyy-MM-dd",
        "backups": 5
      }
    },
    "categories": {
      "default": {
        "appenders": ["system"],
        "level": "info"
      }
    }
  }
}
