{
  "log4js": {
    "appenders": {
      "system": {
        "type": "file",
        "filename": "./log/system.log",
        "maxLogSize": 10485760,
        "backups": 1
      }
    },
    "categories": {
      "default": {
        "appenders": ["system"],
        "level": "info"
      }
    }
  }
}
